{
  "name": "K-link",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [ "notifications", "storage" ],
  "description": "Link everyone on the page!",
  "content_scripts" : [{
      "matches" : ["http://*/*"],
      "js" : ["inject.js"]
  }],
	"browser_action": {
		"default_popup":"popup.html"
	},
  "icons": {
    "32": "Link.png",
    "48": "Link.png"
  }
}
